{
  "name": "eldramatic-webchat",
  "version": "0.0.1",
  "type": "module",
  "description": "A webchat component for eldramatic",
  "keywords": [
    "eldramatic",
    "webchat",
    "eldramatic-webchat"
  ],
  "author": "Dusan Djordjevic <theneura@live.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/eldramatic-webchat.umd.cjs",
  "module": "./dist/eldramatic-webchat.js",
  "exports": {
    ".": {
      "import": "./dist/eldramatic-webchat.js",
      "require": "./dist/eldramatic-webchat.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djordjevicDusan/eldramatic-webchat.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.32",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-lib-inject-css": "^1.3.0"
  }
}